            </column>
        </createTable>
    </changeSet>
    <changeSet author="lostEly" id="1627026225178-12">
        <createTable tableName="procedure">
            <column autoIncrement="true" name="procedure_id" type="BIGINT">
                <constraints primaryKey="true" primaryKeyName="procedure_pkey"/>
            </column>
            <column name="cost" type="FLOAT8">
                <constraints nullable="false"/>
            </column>
            <column name="duration" type="TIME WITHOUT TIME ZONE">
                <constraints nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="equipment_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet author="lostEly" id="1627026225178-13">
        <createTable tableName="role">
            <column autoIncrement="true" name="role_id" type="BIGINT">
                <constraints primaryKey="true" primaryKeyName="role_pkey"/>
            </column>
            <column name="name" type="VARCHAR(255)">
